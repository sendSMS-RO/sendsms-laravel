{
    "name": "sendsms/sendsms-laravel",
    "description": "Use our SMS shipping solution to deliver the right information at the right time.",
    "license": "license",
    "authors": [
        {
            "name": "Radu Vasile Catalin",
            "email": "catalin@sendsms.ro"
        }
    ],
    "homepage": "https://github.com/sendsms/sendsms-laravel",
    "keywords": ["Laravel", "SendsmsLaravel"],
    "require": {
        "illuminate/support": "~7|~8"
